        "def calc_price_including_tax(price, tax_rate):\n",
        "  # 消費税を計算\n",
        "    tax = price * tax_rate / 100\n",
        "  # 支払金額( 税込み )を算出\n",
        "    total_price = price + tax\n",
        "  # 呼び出し先に計算結果を返す\n",
        "    return total_price\n",
        "# 関数を呼び出す\n",
        "calc_price_including_tax(110, 10)"
      ]
    }
